  <td>
    <% place.sectors.each do |sector| %> 
      <%= link_to sector.name, sector %>
    <%end%>

  </td>
  <td><%= place.capacity %></td>
  <td>
    <%= link_to 'Detalhes', place, class: 'btn btn-info btn-small' %>
    <%= link_to 'Editar', edit_place_path(place), class: 'btn btn-primary btn-small'  %>
    <%= link_to 'Excluir', place, method: :delete, data: { confirm: 'VocÃª tem certeza que deseja deletar esse Tipo de Sala?' }, class: 'btn btn-danger btn-small'  %>
  </td>
</tr>