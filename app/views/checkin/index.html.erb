<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th colspan="10">
        Salas
      </th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |places| %>
      <tr>
        <% places.each do |place| %>
          <td>
            <% if place.not_nil? %>
              <%= link_to checkin_details_path(place), class: 'place-checkin' do %>
                <%= place.code %>
                <%= place.status_icon %>
              <% end %>
            <% end %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

